            "delete": {
                "tags": [
                    "appointment"
                ],
                "summary": "Delete appointment record",
                "description": "Deletes an appointment record",
                "operationId": "deleteAppointment",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "Successfully deleted"
                    }
                },
                "security": [
                    {
                        "seki": []
                    }
                ]
            }
        },
        "/diagnosis": {
            "post": {
                "tags": [
                    "diagnosis"
                ],
                "summary": "Store diagnosis record",
                "description": "Stores an diagnosis record",
