        },
        "version": "1.0.3"
    },
    "servers": [
        {
            "url": "/api"
        }
    ],
    "tags": [
        {
            "name": "appointment",
            "description": "Relating to appointments"
        },
        {
            "name": "diagnosis",
            "description": "Relating to cancer diagnosis"
        },
        {
            "name": "patient",
            "description": "Relating to patients"
        }
