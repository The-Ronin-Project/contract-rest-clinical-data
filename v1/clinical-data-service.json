                "tags": [
                    "patient"
                ],
                "summary": "Searches for patients by name or mrn",
                "description": "Get a list of patients matching a search parameter",
                "operationId": "searchPatients",
                "parameters": [
                    {
                        "name": "q",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Collection of patients matching the search parameter",
